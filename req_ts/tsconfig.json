{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["es2019", "dom"],
    "module": "commonjs",
    "rootDir": "./",
    "outDir": "./dist",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  }
}
